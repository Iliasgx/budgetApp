        android:name="com.umbrella.budgetapp.Layouts.UpdateFragments.UpdateRecordBasic"
        android:label="data_record_basic"
        tools:layout="@layout/data_record_basic">
        <action
            android:id="@+id/updateRecordBasicToDetails"
            app:destination="@id/updateRecordDetails" />
        <argument
            android:name="record"
            app:argType="com.umbrella.budgetapp.Database.Collections.Record"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
    <fragment
        android:id="@+id/updateRecordDetails"
        android:name="com.umbrella.budgetapp.Layouts.UpdateFragments.UpdateRecordDetails"
