        android:name="com.umbrella.budgetapp.Layouts.Fragments.HomeInformation"
        android:label="fragment_home_information"
        tools:layout="@layout/fragment_home_information"/>
    <fragment
        android:id="@+id/records"
        android:name="com.umbrella.budgetapp.Layouts.Fragments.Records"
        android:label="fragment_records"
        tools:layout="@layout/fragment_recycler_view">
        <action
            android:id="@+id/recordsToUpdateRecordBasic"
            app:destination="@id/updateRecordBasic" />
        <action
            android:id="@+id/recordsToUpdateRecordDetails"
            app:destination="@id/updateRecordDetails" />
    </fragment>
    <fragment
