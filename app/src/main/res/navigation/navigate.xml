        <argument
            android:name="goalID"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="selectName"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="goalTemplateID"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
