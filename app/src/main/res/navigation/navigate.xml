    <fragment
        android:id="@+id/homeAccounts"
        android:name="com.umbrella.budgetapp.Layouts.Fragments.HomeAccounts"
        android:label="fragment_home_accounts"
        tools:layout="@layout/fragment_home_accounts"/>
